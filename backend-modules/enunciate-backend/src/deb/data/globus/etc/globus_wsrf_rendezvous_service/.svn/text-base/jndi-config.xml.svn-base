<?xml version="1.0" encoding="UTF-8"?>
<jndiConfig xmlns="http://wsrf.globus.org/jndi/config">
    <service name="RendezvousFactoryService">
        <resource
            name="home"
            type="org.globus.rendezvous.service.RendezvousHome">
            <resourceParams>
                <parameter>
                    <name>factory</name>
                    <value>org.globus.wsrf.jndi.BeanFactory</value>
                </parameter>
                <parameter>
                    <name>resourceClass</name>
                    <value>org.globus.rendezvous.service.RendezvousResourceImpl</value>
                </parameter>
                <parameter>
                    <name>resourceKeyType</name>
                    <value>java.lang.String</value>
                </parameter>
            </resourceParams>
        </resource>
    </service>
</jndiConfig>
